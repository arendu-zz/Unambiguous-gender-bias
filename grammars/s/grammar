S -> NPO_Df 'is' NPR_A '. _OCC_BEFORE_'
S -> NPO_Df_A 'is' NPR '. _OCC_BEFORE_'
S -> NPR_Df 'is' NPO_A '. _OCC_AFTER_'
S -> NPR_Df_A 'is' NPO '. _OCC_AFTER_'
